<div class="col-md-4">
  <img class="card-img-top" src="{{this.photo}}" alt="Card image cap" />
  <div class="card-body">
    <h5 class="card-title">
      {{this.name}}
      {{this.lastName}}
    </h5>
    <p class="card-text">
      {{this.team}}
    </p>
    <h5>
        <div class="container">
          <div class="row">
            <div class="col-md-6"><p>Year</p></div>
            <div class="col-md-6">
              <p>Average</p>
            </div>

            {{#each average}}
              <div class="col-md-6">
                <p>{{year}}</p>
              </div>
              <div class="col-md-6">
                <p>{{points}}</p>
              </div>
            {{/each}}
          </div>
        </div>
    </h5>
  </div>
</div>